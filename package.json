{
  "name": "react-formutil",
  "version": "0.5.6-beta.6",
  "description": "Happy to build the forms in React ^_^",
  "main": "dist/react-formutil.cjs.js",
  "module": "dist/react-formutil.esm.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "BABEL_ENV=production ./node_modules/.bin/babel src -d lib",
    "pack": "NODE_ENV=development rollup -c && node_modules/.bin/copy ./npm/*.js ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiqiboy/react-formutil.git"
  },
  "keywords": [
    "react",
    "react-components",
    "react-form",
    "react-formutil",
    "forms",
    "create-form"
  ],
  "author": "qiqiboy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/qiqiboy/react-formutil/issues"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "helpers": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "homepage": "https://github.com/qiqiboy/react-formutil#readme",
  "peerDependencies": {
    "react": ">15.0.0",
    "prop-types": ">15.0.0"
  },
  "types": "./index.d.ts",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "babel-preset-react-app": "^7.0.2",
    "copy": "^0.3.2",
    "rollup": "^1.9.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.1",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^4.0.4"
  },
  "dependencies": {
    "create-react-context": "^0.2.3",
    "hoist-non-react-statics": "^3.2.1",
    "react-fast-compare": "^2.0.4",
    "warning": "^4.0.2"
  }
}
