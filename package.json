{
  "name": "react-formutil",
  "version": "0.5.6-beta.4",
  "description": "Happy to build the forms in React ^_^",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "BABEL_ENV=production ./node_modules/.bin/babel src -d lib",
    "pack:dev": "NODE_ENV=development webpack --config webpack.config.js",
    "pack:prod": "NODE_ENV=production webpack --config webpack.config.js",
    "pack": "npm run build && npm run pack:dev && npm run pack:prod && node_modules/.bin/copy ./index.js ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiqiboy/react-formutil.git"
  },
  "keywords": [
    "react",
    "react-components",
    "react-form",
    "react-formutil",
    "forms",
    "create-form"
  ],
  "author": "qiqiboy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/qiqiboy/react-formutil/issues"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "helpers": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "homepage": "https://github.com/qiqiboy/react-formutil#readme",
  "peerDependencies": {
    "react": ">15.0.0",
    "prop-types": ">15.0.0"
  },
  "types": "./index.d.ts",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "babel-loader": "^8.0.5",
    "babel-preset-react-app": "^7.0.2",
    "copy": "^0.3.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "create-react-context": "^0.2.3",
    "hoist-non-react-statics": "^3.2.1",
    "react-fast-compare": "^2.0.4",
    "warning": "^4.0.2"
  }
}
